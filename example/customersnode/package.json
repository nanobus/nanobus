    "@types/jest": "26.0.24",
    "@types/node": "16.4.10",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"

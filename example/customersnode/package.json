{
  "name": "customersnode",
  "version": "1.0.0",
  "description": "A node.js customers nanoprocess",
  "repository": {
    "type": "git",
    "url": "https://github.com/nanobus/nanobus.git"
  },
  "keywords": [
    "NanoBus",
    "TypeScript",
    "nodejs"
  ],
  "author": "Phil Kedy",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "postinstall": "chmod +x ./scripts/dev.sh",
